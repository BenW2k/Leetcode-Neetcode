def kidsWithCandies(candies, extraCandies):
    """
    :type candies: List[int]
    :type extraCandies: int
    :rtype: List[bool]
    """
    most = 0
    res = []
    for i in range(len(candies)):
        if candies[i] > most:
            most = candies[i]
    for i in range(len(candies)):
        print(candies[i] + extraCandies)
        print("most:" + str(most))
        print("i:" + str(i))

        if candies[i] + extraCandies >= most:
            res.append("true")
        else:
            print("I am false")
            res.append("false")
    return res
candies = [2,3,5,1,3]
extraCandies = 3
kidsWithCandies(candies, extraCandies)
